{
  "name": "web3token",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "tsc -w",
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "ts-node src/test.ts"
  },
  "dependencies": {
    "ms": "^2.1.3",
    "uuid": "^8.3.2",
    "web3": "^1.7.4"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/uuid": "^8.3.4",
    "typescript": "^4.8.3"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@ethereumjs/util": "^8.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ethereumjs": {
      "import": "./dist/ethereumjs/index.mjs",
      "require": "./dist/ethereumjs/index.js",
      "default": "./dist/ethereumjs/index.js"
    }
  }
}
